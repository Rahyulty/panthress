    "build": "rimraf ./dist && tsc",
    "start": "nodemon --watch dist -x 'node dist/index.js'",
    "prod": "NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@sentry/node": "^7.46.0",
    "axios": "^1.3.4",
    "discord.js": "^14.9.0",
