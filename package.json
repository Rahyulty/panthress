{
  "name": "panthress",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "dev": "ts-node ./src/index.ts",
    "watch:dev": "nodemon --watch src -e ts -x 'yarn dev'",
